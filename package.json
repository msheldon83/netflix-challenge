{
  "name": "netflix-challenge",
  "version": "1.0.0",
  "description": "Netflix interview challenge",
  "repository": {
    "type": "git",
    "url": "https://github.com/msheldon83/netflix-challenge"
  },
  "scripts": {
    "start": "run-s  buildui start:server",
    "debug:server": "run-p start:server lint:watch test:watch",
    "start:server": "babel-node src/server/index.js",
    "lint": "esw webpack.config.* src buildScripts",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "devui": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "buildui": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "author": "Mike Sheldon",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.2",
    "eventsource": "^1.0.5",
    "uuid": "^3.1.0",
    "vue": "^2.4.4",
    "vue-resource": "^1.3.4"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "6.2.5",
    "babel-preset-env": "^1.6.0",
    "babel-register": "6.16.3",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "eslint": "4.18.2",
    "eslint-plugin-import": "2.0.1",
    "eslint-watch": "2.1.14",
    "express": "4.14.0",
    "file-loader": "^1.1.4",
    "jest": "^21.2.1",
    "npm-run-all": "3.1.1",
    "open": "0.0.5",
    "testdouble": "^3.2.6",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}
